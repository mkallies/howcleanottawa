<% include partials/header %>

  <div class="container">

    <div class="row text-center results">
      <% results.forEach(function(result){ %>
        <div class='col-md-4'>
          <div class="panel panel-default">
            <div class="panel-heading">
              <h3 class="panel-title"><%= result.fs_fnm %></h3>
            </div>
            <div class="panel-body">
              <%= result.fs_fsf + ' ' + result.fs_fss %>
                <a class='btn btn-info center-block' href='/api/ottawa/<%= result.fs_fdid %>'>Info</a>
            </div>
          </div>
        </div>
        <% }) %>
    </div>
  </div>


  <% include partials/footer %>
